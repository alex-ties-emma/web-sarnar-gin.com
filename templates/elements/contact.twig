<div class="eds-contact {{ content.spacing_bottom }}">
    <div class="eds-contact__wrapper eds-width--full">

        <div class="eds-contact__content">
            <div class="eds-contact__header">
                <span>{{ content.headline }}</span>
            </div>

            <div id="request">

            </div>
        </div>

        <div class="eds-contact__image">
            {{ content.image|optimizeImage([
                {
                    'imageWidth': '420',
                    'mediaMinWidth': '320',
                    'aspect_ratio': '4x3',
                    'compression': '5'
                },
                {
                    'imageWidth': '920',
                    'mediaMinWidth': '700',
                    'aspect_ratio': '4x3',
                    'compression': '5'
                },
                {
                    'imageWidth': '1224',
                    'mediaMinWidth': '1024',
                    'aspect_ratio': '1x1',
                    'compression': '5'
                },
                {
                    'imageWidth': '1400',
                    'mediaMinWidth': '1224',
                    'aspect_ratio': '1x1',
                    'compression': '5'
                }
            ]) }}
        </div>


    </div>
</div>