<div class="eds-contact">
    <div class="eds-contact__wrapper">

        <div class="eds-contact__content">
            <div id="form">

            </div>
        </div>

        <div class="eds-contact__image">
            {{ content.image|optimizeImage([
                {
                    'imageWidth': '420',
                    'mediaMinWidth': '320',
                    'aspect_ratio': '4x3',
                    'compression': '5'
                },
                {
                    'imageWidth': '920',
                    'mediaMinWidth': '700',
                    'aspect_ratio': '4x3',
                    'compression': '5'
                },
                {
                    'imageWidth': '1224',
                    'mediaMinWidth': '1024',
                    'aspect_ratio': '16x10',
                    'compression': '5'
                },
                {
                    'imageWidth': '1400',
                    'mediaMinWidth': '1224',
                    'aspect_ratio': '16x10',
                    'compression': '5'
                }
            ]) }}
        </div>


    </div>
</div>